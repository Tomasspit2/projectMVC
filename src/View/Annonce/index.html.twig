{% extends 'layout.html.twig' %}

{% block content %}
    <h1>Items</h1>
    <ul>
        {% for annonce in annonces %}
            <li><a href="/annonce/show?id={{ annonce.id }}">{{ annonce.id }}. {{ annonce.titre_annonce }}</a></li>
        {% else %}
            <li>Nothing to display</li>
        {% endfor %}
    </ul>
    <a href="/annonce/add">Ajouter</a>
{% endblock %}
